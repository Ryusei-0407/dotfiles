{
  "coc.preferences.formatOnSaveFiletypes": [
    "html",
    "json",
    "yaml",
    "cpp",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "graphql",
    "css",
    "python",
    "go",
    "rust",
    "tf",
    "markdown"
  ],
  "prettier.formatterPriority": 1,
  "prettier.tabWidth": 4,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "languageserver": {
    "rust": {
      "command": "rust-analyzer",
      "filetypes": ["rust"],
      "rootPatterns": ["Cargo.toml"]
    },
    "graphql": {
      "command": "graphql-lsp",
      "args": ["server", "-m", "stream"],
      "filetypes": ["typescript", "typescriptreact", "graphql"]
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    },
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {}
    }
  },
  "files.associations": {
    "json": "jsonc",
    "yaml": "yaml",
    "yml": "yaml",
    "*.svelte": "html",
    "tf": "terraform",
    "tfvars": "terraform",
    "tfstate": "terraform",
    "tfstate.backup": "terraform",
    "tfstate.delta": "terraform",
    "tfstate.lock": "terraform",
    "tfstate.lru": "terraform",
    "tfstate.windows": "terraform",
    "tfstate.windows.backup": "terraform",
    "tfstate.windows.delta": "terraform",
    "tfstate.windows.lock": "terraform",
    "tfstate.windows.lru": "terraform",
    "tfstate.windows.windows": "terraform",
    "tfstate.windows.windows.backup": "terraform",
    "tfstate.windows.windows.delta": "terraform",
    "tfstate.windows.windows.lock": "terraform",
    "tfstate.windows.windows.lru": "terraform",
    "tfstate.windows.windows.windows": "terraform",
    "tfstate.windows.windows.windows.backup": "terraform",
    "tfstate.windows.windows.windows.delta": "terraform",
  },
  "svelte.enable-ts-plugin": true
}
